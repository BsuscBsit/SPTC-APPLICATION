    <!-- Define the icon as a UserControl or DataTemplate -->
    <Viewbox x:Key="YourIconKey" Width="56" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Canvas Height="14" Width="14">
            <!-- Icon Paths and Shapes Go Here -->
